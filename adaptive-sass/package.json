{
  "name": "adaptive-sass",
  "version": "1.0.0",
  "description": "online store",
  "main": "index.js",
  "scripts": {
    "scss:build-main": "node-sass src/scss/styles.scss -o css",
    "scss:build-media": "node-sass src/scss/media.scss -o css",
    "scss:watch-main": "node-sass --watch src/scss/styles.scss -o css",
    "scss:watch-media": "node-sass --watch src/scss/media.scss -o css",
    "build-main": "npm run scss:build-main && npm run scss:watch-main",
    "build-media": "npm run scss:build-media && npm run scss:watch-media"
  },
  "author": "Sergii Mykyteiek <mikiteek@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "node-sass": "^4.14.1"
  }
}
